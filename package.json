		"eslint-plugin-node": "^9.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.0",
		"gtfs-to-leveldb": "^0.2.0",
		"hafas-osm-line-colours": "^0.1.1",
		"level": "^5.0.1",
		"lodash": "^4.17.11",
		"osm-transit-lines": "^0.2.0"
	},
	"engines": {
		"node": ">=8"
	},
